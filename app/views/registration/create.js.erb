$("#flash_holder").html( "<%= j render( "shared/flash_messages" ) %>" );
$(".flash_msg").slideDown( "slow" )

document.hide_spinner( );

<% if @user.errors.count == 0 %>
	$("#additional_fields").html("<%= j( render( 'home/login' ) ) %>");
	$("#home_form").attr( "action", "<%= sessions_path %>" );
	document.show_additional_fields( );
<% end %>